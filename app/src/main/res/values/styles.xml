<!--suppress AndroidDomInspection -->
<resources xmlns:tools="http://schemas.android.com/tools">

    <!-- Base application theme. -->

    <style name="VyndStyle" parent="Theme.AppCompat.NoActionBar">

        <item name="android:statusBarColor">@color/colorPrimary</item>
        <item name="android:contextPopupMenuStyle" tools:targetApi="n">@style/popup</item>
        <item name="android:timePickerDialogTheme">@style/Dialog.timepicker</item>

        <item name="android:windowBackground">@color/backgroundColor</item>
    </style>

    <style name="Dialog.timepicker" parent="Theme.AppCompat.Light.Dialog">
        <item name="android:showText">false</item>
        <item name="colorControlActivated">@color/colorPrimary</item>
        <item name="colorAccent">@color/colorPrimary</item>
        <item name="android:textColorPrimary">@color/blackblack</item>
        <item name="android:textColorSecondary">@color/teal_color</item>
    </style>
    <style name="redchec">
        <item name="colorAccent">@color/colorPrimary</item>
    </style>
    <style name="popup" parent="Widget.AppCompat.Light.PopupMenu">
        <item name="android:textColor">@color/colorPrimary</item>
        <item name="android:itemBackground">@color/white</item>
    </style>

    <style name="SwitchTheme" parent="Widget.AppCompat.CompoundButton.Switch">
        <item name="android:colorControlActivated">@color/colorPrimary</item>
        <item name="colorSwitchThumbNormal">@color/grey_text_color</item>
        <item name="android:colorForeground">@color/grey_line</item>
    </style>

    <style name="VyndStyle.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="VyndStyle.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />


    <style name="Theme.App.Edittext" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorControlNormal">@color/white</item>
        <item name="colorControlActivated">@color/teal_color</item>
    </style>

    <style name="MainActionBar.Popup" parent="ThemeOverlay.AppCompat.Light">
        <item name="android:colorBackground">@color/white</item>
        <item name="android:textColor">@color/colorPrimary</item>
    </style>

    <style name="AlertDialogCustom" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="android:colorAccent">@color/colorPrimary</item>
        <item name="android:textColorPrimary">@color/blackTextColor</item>
        <item name="android:background">@color/white</item>


    </style>
    <style name="AppCompatAlertDialogStyle" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="colorAccent">@color/colorPrimary</item>
        <item name="android:textColorPrimary">#000000</item>
        <item name="android:background">@color/white</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowNoTitle">true</item>
    </style>
    <style name="AppCompatAlertDialogStyleSuggest" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="colorAccent">@color/colorPrimary</item>
        <item name="android:textColorPrimary">#000000</item>
        <item name="android:background">@color/transparent</item>
        <item name="android:windowNoTitle">true</item>
    </style>
    <style name="CheckBoxGray" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorAccent">@color/white_disabled</item>
        <item name="colorControlNormal">@color/gray_2</item>
        <item name="colorControlActivated">@color/colorPrimary</item>
        <item name="colorControlHighlight">@color/colorPrimary</item>
        <item name="android:colorActivatedHighlight">@color/colorPrimary</item>
        <item name="android:textColorSecondary">@color/white</item>
    </style>


    <color name="white_disabled">#80FFFFFF</color>
</resources>
