<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/item_business"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:focusable="true"
    android:focusableInTouchMode="true"
    tools:context="com.vyndsolutions.vyndteam.activities.ItemBusinessActivity">

    <RelativeLayout
        android:id="@+id/rl_default_item_header"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="10dp">

        <com.vyndsolutions.vyndteam.widgets.images.NetworkImageView
            android:id="@+id/item_business_cover_photo"
            android:layout_width="match_parent"
            android:layout_height="125dp"
            android:layout_alignParentStart="true"
            android:background="@color/colorPrimary"
            android:scaleType="centerCrop" />

        <ImageView
            android:id="@+id/item_profil_return_default"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:paddingRight="5dp"
            android:paddingTop="5dp"
            android:paddingBottom="5dp"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="15dp"
            android:src="@drawable/ic_return" />

        <LinearLayout
            android:id="@+id/item_profil_photo_default"
            android:layout_width="84dp"
            android:layout_height="84dp"
            android:layout_below="@id/item_profil_return_default"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="19dp"
            android:background="@drawable/background_camera_photo"
            android:gravity="center">

            <com.vyndsolutions.vyndteam.widgets.images.NetworkImageView
                android:id="@+id/img_profil"
                android:layout_width="46dp"
                android:layout_height="46dp"
                android:src="@drawable/ic_add_photo_white" />

            <com.vyndsolutions.vyndteam.widgets.images.NetworkImageView
                android:id="@+id/img_profil_visible"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:visibility="gone" />
        </LinearLayout>

        <ImageView
            android:id="@+id/more_item_business"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:paddingLeft="5dp"
            android:paddingTop="5dp"
            android:paddingBottom="5dp"
            android:layout_alignParentRight="true"
            android:layout_marginRight="16dp"
            android:layout_marginTop="15dp"
            android:src="@drawable/ic_more_white" />

        <LinearLayout
            android:id="@+id/add_cover_photo"
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:visibility="gone"
            android:layout_marginLeft="128dp"
            android:layout_marginTop="48dp"
            android:layout_toRightOf="@id/item_profil_photo_default"
            android:background="@drawable/background_item_add_photo"
            android:gravity="center_vertical"

            >

            <ImageView
                android:layout_width="27dp"
                android:layout_height="27dp"
                android:layout_marginLeft="5dp"
                android:src="@drawable/ic_add_photo_white" />

            <com.vyndsolutions.vyndteam.utils.TextStyle.LatoTextView
                android:layout_width="87dp"
                android:layout_height="16dp"
                android:layout_marginLeft="8dp"
                android:fontFamily="@fonts/Lato-Bold"
                android:text="couverture"
                android:textAllCaps="true"
                android:textColor="@color/white"
                android:textSize="13sp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@id/item_business_cover_photo"
            android:layout_marginBottom="4dp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="5dp"
            android:layout_toRightOf="@id/item_profil_photo_default"
            android:gravity="center"
            android:orientation="horizontal">

            <com.vyndsolutions.vyndteam.utils.TextStyle.LatoTextView
                android:id="@+id/nom_item"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                tools:text="@string/resto_name"

                android:textColor="@color/white"
                android:textSize="20sp" />

            <ImageView
                android:id="@+id/iv_claimed"
                android:layout_width="16dp"
                android:layout_height="16dp"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="2dp"
                android:src="@drawable/claim"
                android:visibility="gone" />

        </LinearLayout>

        <com.vyndsolutions.vyndteam.utils.TextStyle.LatoTextView
            android:id="@+id/item_subcategory"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/item_business_cover_photo"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="3dp"
            android:layout_toRightOf="@id/item_profil_photo_default"
            android:text="@string/restaurant"
            android:textColor="@color/grey_text_color"
            android:textSize="14sp" />

        <LinearLayout
            android:id="@+id/valide"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_below="@id/item_subcategory"
            android:layout_marginRight="2dp"
            android:layout_marginTop="3dp"
            android:background="@drawable/validate_layout"
            android:gravity="center_horizontal"
            android:paddingBottom="1dp"
            android:paddingLeft="11dp"
            android:paddingRight="10dp"
            android:paddingTop="1dp"
            android:visibility="gone">

            <com.vyndsolutions.vyndteam.utils.TextStyle.LatoTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@fonts/Lato-Bold"
                android:text="valide"
                android:textColor="@color/white"
                android:textSize="12sp" />

        </LinearLayout>

        <LinearLayout
            android:visibility="gone"
            android:id="@+id/non_valide"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_below="@id/item_business_cover_photo"
            android:layout_marginRight="2dp"
            android:layout_marginTop="3dp"
            android:background="@drawable/non_validate_layout"
            android:gravity="center_horizontal"
            android:paddingBottom="1dp"
            android:paddingLeft="11dp"
            android:paddingRight="10dp"
            android:paddingTop="1dp"
            >

            <com.vyndsolutions.vyndteam.utils.TextStyle.LatoTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="non valide"
                android:textColor="@color/white"
                android:textSize="12sp"
                app:fonttype="bold" />

        </LinearLayout>

        <RelativeLayout
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/non_valide"
            android:layout_alignParentRight="true"
            android:layout_marginRight="16dp"
            android:layout_marginTop="10dp"
            android:paddingLeft="16dp"
            android:paddingRight="16dp"
            android:gravity="center"
            android:paddingTop="3dp"
            android:paddingBottom="3dp"
            android:background="@drawable/shapewithgreenborder"
            android:id="@+id/layout_valider_non">
            <com.vyndsolutions.vyndteam.utils.TextStyle.LatoTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/valider"
                android:id="@+id/lbl_valider"
                android:textSize="14sp"
                android:layout_centerVertical="true"
                app:fonttype="regular"
                android:textColor="@color/green_color"/>
            <com.vyndsolutions.vyndteam.widgets.images.NetworkImageView
                android:layout_width="18dp"
                android:layout_height="14dp"

                android:layout_marginLeft="5dp"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/lbl_valider"
                android:src="@drawable/tick_green"/>
        </RelativeLayout>

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="143dp"
        android:visibility="gone">

        <com.vyndsolutions.vyndteam.widgets.images.NetworkImageView
            android:layout_width="match_parent"
            android:layout_height="125dp"
            android:background="@color/colorPrimary" />

        <ImageView
            android:id="@+id/item_profil_return"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_marginLeft="19dp"
            android:layout_marginTop="20dp"
            android:src="@drawable/ic_return" />

        <com.vyndsolutions.vyndteam.widgets.images.NetworkImageView
            android:id="@+id/item_profil_photo"
            android:layout_width="84dp"
            android:layout_height="84dp"
            android:layout_below="@id/item_profil_return"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="19dp"
            android:src="@drawable/empty_business_photo" />

        <ImageView
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_marginLeft="287.2dp"
            android:layout_marginTop="20dp"
            android:layout_toRightOf="@id/item_profil_return"
            android:src="@drawable/ic_more_white" />

        <com.vyndsolutions.vyndteam.utils.TextStyle.LatoTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="96dp"
            android:layout_toRightOf="@id/item_profil_photo"
            android:text="Nom du restaurant"
            android:textColor="@color/white"
            android:textSize="20sp" />

        <com.vyndsolutions.vyndteam.utils.TextStyle.LatoTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="126dp"
            android:layout_toRightOf="@id/item_profil_photo"
            android:text="Restaurant"
            android:textColor="@color/grey_text_color"
            android:textSize="14sp" />

        <LinearLayout
            android:layout_width="54dp"
            android:layout_height="17dp"
            android:layout_marginLeft="204dp"
            android:layout_marginTop="126dp"
            android:layout_toRightOf="@id/item_profil_photo"
            android:background="@drawable/validate_layout"
            android:gravity="center_horizontal">

            <com.vyndsolutions.vyndteam.utils.TextStyle.LatoTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@fonts/Lato-Bold"
                android:text="validé"
                android:textColor="@color/white"
                android:textSize="12sp" />

        </LinearLayout>

    </RelativeLayout>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"

        >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/layout_business_profile_loading"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="10dp"
                android:background="@color/gray_0"
                android:orientation="vertical"

                >
                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="12dp"
                    android:adjustViewBounds="true"

                    android:src="@drawable/business_profile_loading" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/layout_business_profile_content"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:visibility="gone"


                >

                <com.vyndsolutions.vyndteam.utils.TextStyle.LatoTextView
                    android:id="@+id/item_region"
                    android:layout_width="wrap_content"
                    android:layout_height="17dp"
                    android:layout_marginLeft="16dp"
                    android:layout_marginTop="10dp"
                    android:text="Bardo"
                    android:textColor="@color/teal_color"
                    android:textSize="14sp" />

                <com.vyndsolutions.vyndteam.utils.TextStyle.LatoTextView
                    android:id="@+id/item_address"
                    android:layout_width="match_parent"
                    android:layout_height="15dp"
                    android:layout_marginLeft="16dp"
                    android:text="---"
                    android:textColor="@color/grey_text_color"
                    android:textSize="11sp" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="17dp"
                    android:layout_marginLeft="16dp"
                    android:layout_marginTop="10dp"
                    android:orientation="horizontal">

                    <com.vyndsolutions.vyndteam.utils.TextStyle.LatoTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Nombre de vues :"
                        android:textColor="@color/grey_text_color"
                        android:textSize="14sp" />

                    <com.vyndsolutions.vyndteam.utils.TextStyle.LatoTextView
                        android:layout_width="match_parent"
                        android:layout_height="17dp"
                        android:id="@+id/tv_nbr_view"
                        android:layout_marginLeft="5dp"
                        tools:text="55"
                        android:textColor="@color/blackTextColor" />

                </LinearLayout>


                <LinearLayout
                    android:visibility="gone"
                    android:id="@+id/happy_hours"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginTop="10dp"
                    android:orientation="vertical"
                    >



                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <ImageView
                            android:layout_width="20dp"
                            android:layout_height="50dp"
                            android:layout_marginLeft="17dp"
                            android:layout_marginTop="-17dp"
                            android:src="@drawable/ic_happy_hour_blue" />

                        <com.vyndsolutions.vyndteam.utils.TextStyle.LatoTextView
                            android:layout_width="match_parent"
                            android:layout_height="19dp"
                            android:layout_marginLeft="6dp"
                            android:text="Happy hours :"
                            android:textColor="@color/teal_color" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <android.support.v7.widget.RecyclerView
                            android:id="@+id/rv_happy_hours"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal" />
                    </LinearLayout>
                </LinearLayout>


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginTop="6dp"
                    android:background="@color/grey_line" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="15dp"
                    android:background="@color/gray_4"
                    android:orientation="vertical"></LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/grey_line"
                    android:orientation="horizontal" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/grey_line"
                    android:orientation="horizontal" />

                <RelativeLayout
                    android:id="@+id/adress_layout"
                    android:layout_width="match_parent"
                    android:layout_height="39dp"
                    android:layout_gravity="center_vertical">

                    <com.vyndsolutions.vyndteam.utils.TextStyle.LatoTextView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="8dp"
                        android:fontFamily="@fonts/Lato-Regular"
                        android:gravity="center_vertical"
                        android:text="Adresse et localisation"
                        android:textColor="#363636 " />

                    <ImageView
                        android:id="@+id/warning_adress_item"
                        android:layout_width="18dp"
                        android:layout_height="18dp"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="19dp"
                        android:src="@drawable/text_warning_icon" />

                </RelativeLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="#B1BAC4 "
                    android:orientation="vertical"></LinearLayout>

                <LinearLayout
                    android:id="@+id/layout_photo_gallerie"
                    android:layout_width="match_parent"
                    android:layout_height="39dp"
                    android:layout_weight="1">

                    <com.vyndsolutions.vyndteam.utils.TextStyle.LatoTextView
                        android:layout_width="0dp"
                        android:layout_height="39dp"
                        android:layout_marginLeft="8dp"
                        android:layout_weight="3"
                        android:fontFamily="@fonts/Lato-Regular"
                        android:gravity="center_vertical"

                        android:text="Gallerie de photos"
                        android:textColor="#363636 " />

                    <com.vyndsolutions.vyndteam.utils.TextStyle.LatoTextView
                        android:id="@+id/nombre_photo"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="20dp"
                        android:layout_weight="1"
                        android:gravity="end"

                        android:text="0"
                        android:textColor="@color/colorPrimary"
                        android:textSize="20sp" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/grey_line"
                    android:orientation="horizontal" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="15dp"
                    android:background="@color/gray_4"
                    android:orientation="vertical"></LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/grey_line"
                    android:orientation="horizontal" />

                <LinearLayout
                    android:id="@+id/ll_horaires"
                    android:layout_width="match_parent"
                    android:layout_height="39dp"
                    android:layout_weight="1"
                    android:gravity="center_vertical">

                    <com.vyndsolutions.vyndteam.utils.TextStyle.LatoTextView
                        android:layout_width="0dp"
                        android:layout_height="39dp"
                        android:layout_weight="3"
                        android:fontFamily="@fonts/Lato-Regular"
                        android:gravity="center_vertical"
                        android:padding="10dp"
                        android:text="Horaires"
                        android:textColor="#363636 " />

                    <ImageView
                        android:layout_width="18dp"
                        android:layout_height="18dp"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="19dp"
                        android:id="@+id/iv_warning_horaire"
                        android:src="@drawable/text_warning_icon" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="#B1BAC4 "
                    android:orientation="vertical"></LinearLayout>

                <LinearLayout
                    android:id="@+id/layout_menu"
                    android:layout_width="match_parent"
                    android:layout_height="39dp"
                    android:gravity="center_vertical">

                    <com.vyndsolutions.vyndteam.utils.TextStyle.LatoTextView
                        android:layout_width="0dp"
                        android:layout_height="39dp"
                        android:layout_weight="3"
                        android:fontFamily="@fonts/Lato-Regular"
                        android:gravity="center_vertical"
                        android:padding="10dp"
                        android:text="Menu"
                        android:textColor="#363636 " />

                    <ImageView
                        android:layout_width="18dp"
                        android:layout_height="18dp"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="19dp"
                        android:id="@+id/iv_warning_menu"
                        android:src="@drawable/text_warning_icon" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="#B1BAC4"
                    android:orientation="vertical"></LinearLayout>

                <LinearLayout
                    android:id="@+id/informations"
                    android:layout_width="match_parent"

                    android:layout_height="39dp"
                    android:gravity="center_vertical">

                    <com.vyndsolutions.vyndteam.utils.TextStyle.LatoTextView
                        android:layout_width="0dp"
                        android:layout_height="39dp"
                        android:layout_weight="3"
                        android:fontFamily="@fonts/Lato-Regular"
                        android:gravity="center_vertical"
                        android:padding="10dp"
                        android:text="informations supplémentaire"
                        android:textColor="#363636 " />

                    <ImageView
                        android:id="@+id/warning_infos_item"
                        android:layout_width="18dp"
                        android:layout_height="18dp"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="19dp"
                        android:src="@drawable/text_warning_icon" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/grey_line"
                    android:orientation="horizontal" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="15dp"
                    android:background="@color/gray_4"
                    android:orientation="vertical"></LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/grey_line"
                    android:orientation="horizontal" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="39dp"
                    android:layout_weight="1">

                    <com.vyndsolutions.vyndteam.utils.TextStyle.LatoTextView
                        android:layout_width="0dp"
                        android:layout_height="39dp"
                        android:layout_weight="2"
                        android:fontFamily="@fonts/Lato-Regular"
                        android:gravity="center_vertical"
                        android:padding="10dp"
                        android:text="Téléphone"
                        android:textColor="#363636 "
                        android:textSize="16dp" />

                    <EditText
                        android:id="@+id/tel"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="3"
                        android:fontFamily="@fonts/Lato-Regular"
                        android:hint="+216 xx xxx xxx"
                        android:inputType="phone"
                        android:textColorHint="#B1BAC4 "
                        android:textSize="16sp"
                        android:theme="@style/Theme.App.Edittext" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="#B1BAC4 "
                    android:orientation="vertical"></LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="39dp"
                    android:layout_weight="1">

                    <com.vyndsolutions.vyndteam.utils.TextStyle.LatoTextView
                        android:layout_width="0dp"
                        android:layout_height="39dp"
                        android:layout_weight="2"
                        android:fontFamily="@fonts/Lato-Regular"
                        android:gravity="center_vertical"
                        android:padding="10dp"
                        android:text="Code postal"
                        android:textColor="#363636 "
                        android:textSize="16dp" />

                    <EditText
                        android:id="@+id/code_postal"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="3"
                        android:hint="xxxx"
                        android:inputType="number"
                        android:textColorHint="#B1BAC4 "
                        android:textSize="16dp"
                        android:theme="@style/Theme.App.Edittext" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="#B1BAC4 "
                    android:orientation="vertical"></LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1">

                    <com.vyndsolutions.vyndteam.utils.TextStyle.LatoTextView
                        android:layout_width="0dp"
                        android:layout_height="39dp"
                        android:layout_weight="2"
                        android:fontFamily="@fonts/Lato-Regular"
                        android:gravity="center_vertical"
                        android:padding="10dp"
                        android:text="Description"
                        android:textColor="#363636 "
                        android:textSize="16dp" />

                    <EditText
                        android:id="@+id/description"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="3"
                        android:fontFamily="@fonts/Lato-Regular"
                        android:hint="Description"
                        android:textColorHint="#B1BAC4 "
                        android:textSize="16dp"
                        android:theme="@style/Theme.App.Edittext" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/grey_line"
                    android:orientation="horizontal" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="15dp"
                    android:background="@color/gray_4"
                    android:orientation="vertical"></LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:orientation="horizontal" />
            </LinearLayout>
        </LinearLayout>
    </ScrollView>
</LinearLayout>


