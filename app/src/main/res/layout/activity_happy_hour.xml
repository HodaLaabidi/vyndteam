                                android:layout_marginLeft="45dp"
                                android:layout_toRightOf="@+id/lbl_mardi"
                                android:textColor="@color/gray_2"
                                android:textSize="16sp"
                                app:fonttype="bold"
                                android:text="de" />

                            <com.vyndsolutions.vyndteam.utils.TextStyle.LatoTextView
