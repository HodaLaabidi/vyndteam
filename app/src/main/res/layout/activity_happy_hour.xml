<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@color/gray_0"
    android:layout_height="match_parent"
    tools:context="com.vyndsolutions.vyndteam.activities.HappyHourActivity">
    <android.support.v7.widget.RecyclerView
        android:id="@+id/rv_happy_hours"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        ></android.support.v7.widget.RecyclerView>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_centerInParent="true"
        android:orientation="vertical">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <android.support.v7.widget.Toolbar
                android:id="@+id/toolbar"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:background="@color/colorPrimary">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="16dp"
                    android:layout_marginRight="16dp"
                    android:baselineAligned="false"
                    android:gravity="center"
                    android:orientation="horizontal"


                    >

                    <RelativeLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"

                        android:layout_weight="0.5"

                        >

                        <com.vyndsolutions.vyndteam.widgets.images.NetworkImageView
                            android:id="@+id/iv_back"
                            android:layout_width="34dp"
                            android:layout_height="34dp"
                            android:paddingBottom="5dp"
                            android:paddingTop="5dp"
                            android:paddingRight="5dp"

                            android:foreground="?attr/selectableItemBackground"
                            android:src="@drawable/ic_back" />

                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="8dp"
                        android:layout_weight="1">

                        <com.vyndsolutions.vyndteam.utils.TextStyle.LatoTextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_centerInParent="true"
                            android:text="@string/happyhours"
                            android:textAlignment="center"
                            android:textColor="@color/white"
                            android:textSize="20sp"
                            app:fonttype="bold" />

                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"

                        android:layout_weight="0.5"

                        >

                        <com.vyndsolutions.vyndteam.widgets.images.NetworkImageView
                            android:id="@+id/iv_edit"
                            android:layout_width="34dp"
                            android:layout_height="34dp"
                            android:paddingBottom="5dp"
                            android:paddingTop="5dp"
                            android:paddingLeft="5dp"

                            android:foreground="?attr/selectableItemBackground"
                            android:layout_alignParentRight="true"
                            android:src="@drawable/edit" />


                    </RelativeLayout>
                </LinearLayout>
            </android.support.v7.widget.Toolbar>
            <android.support.v7.widget.Toolbar
                android:id="@+id/toolbar_update"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/colorPrimary"
                android:layout_alignParentTop="true"
                android:visibility="gone"
                >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="16dp"
                    android:layout_marginRight="16dp"
                    android:baselineAligned="false"
                    android:gravity="center"
                    android:orientation="horizontal"


                    >

                    <RelativeLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"

                        android:layout_weight="0.5"

                        ><com.vyndsolutions.vyndteam.widgets.images.NetworkImageView
                        android:layout_width="24dp"
                        android:layout_height="24dp"
                        android:src="@drawable/ic_back"
                        android:id="@+id/iv_back_update"/>

                    </RelativeLayout>



                    <RelativeLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"

                        android:layout_weight="0.5"

                        >
                        <com.vyndsolutions.vyndteam.utils.TextStyle.LatoTextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@string/sauvgarder"
                            android:textAllCaps="true"
                            app:fonttype="regular"
                            android:textSize="16sp"
                            android:textColor="@color/white"
                            android:layout_centerInParent="true"
                            android:gravity="end|center"
                            android:id="@+id/tv_save"/>


                    </RelativeLayout>
                </LinearLayout>
            </android.support.v7.widget.Toolbar>
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/layout_empty_happy_hour"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:visibility="gone">

            <com.vyndsolutions.vyndteam.utils.TextStyle.LatoTextView
                android:layout_width="260dp"
                android:layout_height="wrap_content"
                android:layout_above="@+id/layout_add_happy_hour"
                android:layout_centerHorizontal="true"
                android:gravity="center"
                android:text="@string/empty_happy_hour"
                android:textColor="@color/bleub"
                android:textSize="18sp"
                app:fonttype="regular"

                />

            <RelativeLayout
                android:id="@+id/layout_add_happy_hour"
                android:layout_width="260dp"
                android:layout_height="40dp"
                android:layout_centerInParent="true"
                android:layout_marginTop="20dp"
                android:background="@drawable/shapeorange">

                <com.vyndsolutions.vyndteam.utils.TextStyle.LatoTextView

                    android:layout_width="260dp"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:gravity="center"
                    android:text="@string/add_empty_happy_hour"
                    android:textAlignment="center"
                    android:textColor="@color/white"
                    android:textSize="18sp" />
            </RelativeLayout>

        </RelativeLayout>
        <RelativeLayout
