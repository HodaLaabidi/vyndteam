                                android:layout_height="wrap_content"
                                android:layout_marginLeft="45dp"
                                android:layout_toRightOf="@+id/lbl_lundi"
                                android:textColor="@color/gray_2"
                                android:textSize="16sp"
                                app:fonttype="bold"
