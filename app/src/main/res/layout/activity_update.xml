                android:layout_alignParentRight="true"
                android:layout_marginRight="16dp"
                android:id="@+id/ic_horaire"
                android:src="@drawable/text_warning"/>

        </RelativeLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="1dp"
