                android:textColor="@color/black"
                android:layout_toRightOf="@+id/lbl_business_description"
                android:layout_marginLeft="15dp"
                android:hint="Description"
                android:textColorHint="@color/gray_2"
                android:layout_marginRight="5dp"
                android:id="@+id/commerceDescription"/>
            <com.vyndsolutions.vyndteam.widgets.images.NetworkImageView
                android:layout_width="17dp"
                android:layout_height="17dp"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:layout_marginRight="16dp"
                android:id="@+id/ic_description"
                android:src="@drawable/text_warning"/>

        </RelativeLayout>
    </LinearLayout>
</android.support.v4.widget.NestedScrollView>
<LinearLayout
    android:layout_width="match_parent"
    android:layout_centerInParent="true"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:visibility="gone"
    android:id="@+id/layout_no_internet_edit">
    <com.vyndsolutions.vyndteam.widgets.images.NetworkImageView
        android:layout_marginTop="10dp"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:src="@drawable/error_loading"
        android:layout_gravity="center"/>
    <com.vyndsolutions.vyndteam.utils.TextStyle.LatoTextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:fonttype="bold"
        android:textSize="14sp"
        android:gravity="center"
        android:layout_marginLeft="50dp"
        android:layout_marginRight="50dp"
        android:text="@string/chargement_gerer"/>
    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginTop="5dp"
        android:gravity="center">
        <com.vyndsolutions.vyndteam.widgets.images.NetworkImageView
            android:layout_width="16dp"
            android:layout_height="16dp"

            android:background="@drawable/arrow_loading"
            android:id="@+id/iv_arrow_loading_accueil"/>
        <com.vyndsolutions.vyndteam.utils.TextStyle.LatoTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@+id/iv_arrow_loading_accueil"
            android:layout_marginLeft="5dp"
            app:fonttype="regular"
            android:textSize="14sp"

            android:text="@string/ressayer"/>
    </RelativeLayout>
</LinearLayout>
<RelativeLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_below="@+id/toolbar"
    android:background="@color/transparent_black_1"
    android:id="@+id/layout_loading"
    android:visibility="gone">
    <ProgressBar
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"/>
</RelativeLayout>

</RelativeLayout>

