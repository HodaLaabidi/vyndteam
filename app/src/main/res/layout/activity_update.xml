                android:layout_width="70dp"
                android:layout_height="wrap_content"
                android:text="@string/label_nom"
                android:layout_centerVertical="true"
                android:id="@+id/lbl_business_name"
                android:textSize="16sp"
                android:textColor="@color/black"
                android:layout_marginLeft="16dp"
                app:fonttype="regular"/>
            <com.vyndsolutions.vyndteam.utils.TextStyle.LatoEditText
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="16sp"
                android:background="@android:color/transparent"

                android:layout_centerVertical="true"
                android:textColor="@color/black"
                android:layout_toRightOf="@+id/lbl_business_name"
                android:layout_marginLeft="15dp"
                android:hint="@string/lbl_nom_commerce"
                android:layout_marginRight="5dp"
                android:textColorHint="@color/gray_2"
                android:layout_toLeftOf="@+id/ic_name"
                android:id="@+id/commerceName"/>
            <com.vyndsolutions.vyndteam.widgets.images.NetworkImageView
                android:layout_width="17dp"
                android:layout_height="17dp"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:layout_marginRight="16dp"
                android:id="@+id/ic_name"
                android:src="@drawable/text_warning"/>

        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/gray_1"/>
        <RelativeLayout
