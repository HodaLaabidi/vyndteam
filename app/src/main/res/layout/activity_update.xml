                android:text="Gallerie de photos"
                android:layout_marginRight="5dp"
                android:id="@+id/lbl_gallerie"/>
            <com.vyndsolutions.vyndteam.widgets.images.NetworkImageView
                android:layout_width="17dp"
                android:layout_height="17dp"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"

                android:layout_marginRight="16dp"
                android:id="@+id/ic_gallerie"
                android:src="@drawable/text_warning"/>

            <com.vyndsolutions.vyndteam.utils.TextStyle.LatoTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/tv_nbr_photo"
                android:textColor="@color/black"
                android:layout_toLeftOf="@+id/ic_gallerie"
                android:textSize="16sp"
                android:visibility="gone"
                tools:text="5"

                android:layout_marginRight="5dp"/>
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:background="@color/white"
            android:paddingTop="8dp"
